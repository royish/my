echo rg_conf_set wbm/session_lifetime 900000000	 > /dev/ttyS0
echo rg_conf_set cwmp/enabled 0			 > /dev/ttyS0
echo rg_conf_set jnet/enabled 0			 > /dev/ttyS0
echo conf set wbm/session_lifetime 900000000	 > /dev/ttyS0
echo conf set cwmp/enabled 0			 > /dev/ttyS0
echo conf set jnet/enabled 0			 > /dev/ttyS0
echo conf set system/network/initial_setup 1	 > /dev/ttyS0
echo conf set wbm/license_agreed 1		 > /dev/ttyS0
echo conf set wbm/is_login_welcome_done 1	 > /dev/ttyS0
echo conf set wbm/is_login_setup_done 1		 > /dev/ttyS0
echo conf set wbm/is_installation_done 1	 > /dev/ttyS0
echo reconf 1					 > /dev/ttyS0
echo conf reconf 1				 > /dev/ttyS0
