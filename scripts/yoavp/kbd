xmodmap -e "keycode 107 = BackSpace" 	# print screen
xmodmap -e "keycode 133 = F13" 		# left win
#xmodmap -e "keycode 135 = Pause" 	# menu

# capslock -> super
xmodmap -e "remove lock = Caps_Lock"
xmodmap -e "keycode 66 = Super_L"
#xmodmap -e "keycode 66 = Tab"
# 116 = right win
xmodmap -e "keycode 134 = Super_L"
xmodmap -e "clear Mod4"
xmodmap -e "add Mod4 = Super_L"

#-heb

# wait & rate
xset r rate 258 59
# no autorepeat for win
xset -r 133
